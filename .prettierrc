{
	"printWidth": 150,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": false,
	"endOfLine": "auto",
	"jsxSingleQuote": true,
	"useTabs": true,
	"tabWidth": 2,
	"arrowParens": "always",
	"bracketSameLine": true,
	"bracketSpacing": true,
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"singleAttributePerLine": true,
	"htmlWhitespaceSensitivity": "ignore",
	"plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-css-order"],
	"attributeGroups": ["$CODE_GUIDE"],
